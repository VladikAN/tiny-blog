    "terser-webpack-plugin": "^1.4.6",
    "testcafe": "^1.20.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.10",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "setupFilesAfterEnv": [
      "<rootDir>/Scripts/setupTests.ts"
    ]
