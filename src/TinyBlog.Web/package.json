    "react-dom": "^16.14.0",
    "react-localization": "^1.0.19",
    "react-redux": "^7.2.9",
    "react-redux-toastr": "^7.6.13",
    "react-router-dom": "^5.3.4",
    "react-showdown": "^1.6.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "typicons.font": "^2.1.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.18",
    "@types/enzyme-adapter-react-16": "^1.0.9",
    "@types/jest": "^24.9.1",
