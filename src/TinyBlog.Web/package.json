    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^3.0.1",
    "jest": "^24.9.0",
